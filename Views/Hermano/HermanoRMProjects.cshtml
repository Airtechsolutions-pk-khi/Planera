@{
    ViewBag.Title = "HermanoRMProjects";
    Layout = "~/Views/Shared/_LayoutHermano.cshtml";
}
<style>
    .if-zoom .image_frame .image_wrapper .image_links {
        display: unset;
    }

    .hc-projects img.scale-with-grid, #Content img {
        height: 251px;
    }

    .image_frame .image_wrapper .image_links, .portfolio_group.masonry-hover .portfolio-item .masonry-hover-wrapper .hover-desc {
        background: rgba(28, 130, 86, 0.80);
    }

        .image_frame .image_wrapper .image_links a:hover {
            background: #ffffff;
            color: rgba(28, 130, 86, 0.80) !important;
        }

    h5 {
        color: #056b39 !important;
    }
</style>
<div id="Content">
    <div class="section full-width center dark flv_sections_53" style="background-image: url(../../assets/HarmanoRepair/images/hr-project-header.jpg);background-size: cover;background-repeat: no-repeat;" data-stellar-background-ratio="0.5">
        <div class="section_wrapper clearfix">
            <div class="items_group clearfix"></div>
            <h2 class="mobView" data-translate="0gal1">
                Gallery
            </h2>
        </div>
    </div>
    <div class="section mcb-section" style="padding-top:80px; padding-bottom:40px; background-color:#ededed">
        <div class="section_wrapper clearfix">
            <!-- One full width row-->
            <div id="Filters" class="column one isotope-filters">
                <ul class="filters_buttons">
                    <li class="label" data-translate="0fb01">
                        Filter by
                    </li>
                    <li class="categories">
                        <a class="open" href="#" data-translate="00h7"><i class="icon-docs"></i>Categories<i class="icon-down-dir"></i></a>
                    </li>
                    <li class="reset">
                        <a class="close" data-rel="*" href="#" data-translate="0sa01"><i class="icon-cancel"></i> Show all</a>
                    </li>
                </ul>
                <div class="filters_wrapper" style="display: block;">
                    <ul class="categories">
                        <li class="current-cat">
                            <a data-rel=".category-home" href="#" data-translate="0rpr1">Repair</a>
                        </li>
                        <li>
                            <a data-rel=".category-appartment" href="#" data-translate="0plm1">Plumbing</a>
                        </li>
                        <li>
                            <a data-rel=".category-shop" href="#" data-translate="0mnt1">Maintainence</a>
                        </li>
                        <li>
                            <a data-rel=".category-photography" href="#" data-translate="0oth1">Others</a>
                        </li>
                        @*<li>
                    <a data-rel=".category-print" href="#">Category 5</a>
                </li>
                <li>
                    <a data-rel=".category-webdesign" href="#">Category 6</a>
                </li>*@
                        <li class="close">
                            <a href="#"><i class="icon-cancel"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="column one column_portfolio hc-projects">
                <div class="portfolio_wrapper isotope_wrapper">
                    <ul class="portfolio_group lm_wrapper isotope grid">
                        <!-- Portfolio item -->
                        @foreach (var item in ViewBag.Gallery)
                        {
                            <li class="portfolio-item isotope-item category-home category-appartment  ">
                                <div class="portfolio-item-fw-bg">
                                    <div class="portfolio-item-fw-wrapper">

                                        <div class="image_frame scale-with-grid">
                                            <div class="image_wrapper">
                                                
                                                <a href="@System.Configuration.ConfigurationManager.AppSettings["Image"].ToString()@item.ImagePath" class="zoom" rel="prettyphoto">
                                                    <div class="mask"></div>
                                                    @*<img width="650" height="650px" src="~/assets/HarmanoRepair/images/hermano-gallery/01.jpg" class="scale-with-grid wp-post-image" />*@
                                                    <img width="650" height="650px" src="@System.Configuration.ConfigurationManager.AppSettings["Image"].ToString()@item.ImagePath" alt="@item.Title" class="scale-with-grid wp-post-image">
                                                </a>
                                                <div class="image_links">
                                                    <a href="@System.Configuration.ConfigurationManager.AppSettings["Image"].ToString()@item.ImagePath" class="zoom" rel="prettyphoto"><i class="icon-search"></i></a>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </li>
                        }
