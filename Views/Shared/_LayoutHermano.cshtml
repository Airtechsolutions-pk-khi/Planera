    <script>
      function SendEmailBookNow() {
            var dataType = 'application/json; charset=utf-8';
            var obj = {
                "FName": $('#fname').val(),
                "LName": $('#lname').val(),
                "Email": $('#email').val(),
                "Phone": $('#phone').val(),
                "Date": $('#date').val(),
                "Service": $('#service').val(),
                "Time": $('#time').val(),
                "Details": $('#details').val()

            }
            $.ajax({
                type: 'POST',
                url: '@Url.Action("SendEmailToAdminBookNow", "Hermano")',
                dataType: 'json',
                contentType: dataType,
                data: JSON.stringify(obj),
                success: function (result) {
                    if (result.Success == true) {
                        $("#myModal").modal({ show: false });
                        swal({
                           title: "We have received you query, our team will get back to you soon!",
                            //text: "",
                            type: "success"
                        });
                        ClearFields()
                    }
                    else {
                        swal({
                            title: "Error !",
                            text: "Error Occured While Sending an Email",
                            type: "error",
                            html: true,
                            showCancelButton: false,
                            confirmButtonColor: "#DD6B55",
                            confirmButtonText: "Ok",
                            closeOnConfirm: false,
                        });
                        ClearFields()
                    }
                }
            });

        }

        function ClearFields() {
            $('#fname').val('');
            $('#lname').val('');
            $('#email').val('');
            $('#Phone').val('');
            $('#service').val('');
            $('#time').val('');
            $('#details').val('');
        }
    </script>
