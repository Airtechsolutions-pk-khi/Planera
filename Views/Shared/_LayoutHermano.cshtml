                jQuery(".padding-left").css("padding-left", "25px")
                jQuery(".padding-right").css("padding-right", "0px")
                jQuery("._arwidth").css("width", "100%")
                jQuery("._mrright").css("margin-right", "0px")
                jQuery(".armgn-top").css("margin-top", "50px")
            }
        });

        function _lang() {
            var currLang = Cookies.get('lang');
            //var currLang = localStorage.getItem("_lang");
            if (currLang == null || currLang == "") {
                Cookies.set('lang', 'en', { expires: 365 });
                jQuery('#_lang').html('عربى');
            }
            if (currLang == "en") {
                Cookies.set('lang', 'ar', { expires: 365 });
                jQuery('#_lang').html('English');
                location.reload();
            }
            if (currLang == "ar") {
                Cookies.set('lang', 'en', { expires: 365 });
                jQuery('#_lang').html('عربى');
                location.reload();
            }
        }
    </script>


