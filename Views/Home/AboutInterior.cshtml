
@{
    ViewBag.Title = "AboutInterior";
    Layout = "~/Views/Shared/_LayoutInterior.cshtml";
}
<div class="sections_group">
    <div class="entry-content">
        <div class="section full-width center dark flv_sections_53" style="background-image: url(../../assets/PlaneraInterior/images/about-us.png);background-size: cover;background-repeat: no-repeat;" data-stellar-background-ratio="0.5">
            <div class="section_wrapper clearfix">
                <div class="items_group clearfix"></div>
                <h2  class="mobView" style="opacity: 0.5;    font-size: 83.5px;    font-weight: bold;    font-style: normal;    color: rgba(255, 255, 255, 0.5);">
                    About
                </h2>
            </div>
        </div>
        <div class="section mcb-section" style="padding-top:80px; padding-bottom:80px; background-image:url(content/interior2/images/home_interior2_bg_section.png); background-repeat:no-repeat; background-position:center;">
            <div class="section_wrapper mcb-section-inner">
                <!-- One Second (1/2) Column -->
                <div class="wrap mcb-wrap one-second column-margin-20px clearfix" style="padding:15% 3% 0 0;">
                    <!-- One Full Row-->
                    <div class="column mcb-column one column_column">
                        <div class="column_attr animate fadeInLeft">
                            <h3>We shape your values through Innovative Architecture & Design Perspectives</h3>
                            <p style="font-size: 17px; text-align: justify;">
                                Planera interior transforms your standards that purely connect with people by marrying unbounded creativity with intelligent utility. Our designs are professional and passionate to meet
                                the ambition of our customers.
                            </p>
                        </div>
                    </div>
                </div>
                <!-- One Second (1/2) Column -->
                <div class="wrap mcb-wrap one-second clearfix">
                    <!-- One Full Row-->
                    <div class="column mcb-column one column_image">
                        <div class="image_frame image_item no_link scale-with-grid aligncenter no_border">
                            <div class="image_wrapper animate fadeInRight">
                                <img class="scale-with-grid" src="~/assets/PlaneraInterior/images/about-section1.png" width="585" height="539" />
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="section mcb-section" style="padding-top:150px; padding-bottom:130px;background:#000">
            <div class="section_wrapper mcb-section-inner">
                <div class="wrap mcb-wrap one valign-top clearfix">
                    <div class="mcb-wrap-inner">
                        <div class="column mcb-column one-fifth column_placeholder">
                            <div class="placeholder">
                                &nbsp;
                            </div>
                        </div>
                        <div class="column mcb-column four-fifth column_column animate" data-anim-type="fadeInRight">
                            @*<div class="column_attr clearfix" style="padding:100px 8% 85px 45%; border: 20px solid #fff; border-radius: 6px">*@
                                <div class="column_attr clearfix" style="padding:47px 8% 32px 43%; border: 20px solid #fff; border-radius: 6px">
                                    <h2 style="color:white">Why Planera Interior</h2>
                                    <hr class="no_line" style="margin:0 auto 10px">
                                    <p style="color:#aaaaaa">
                                        We work on innovations and innovations does creative things to look differently.
                                    </p>

                                    <p style="color:#aaaaaa">
                                        We have a working formula "QRECS". Quality, Reliability, Efficiency, Customer, Services
                                    </p>
                                    <hr class="no_line" style="margin:0 auto 20px">
                                    <a class="button  button_size_2 button_js" href="/interior/project" style="background:#ddd;color:#515150"><span class="button_label">View Our Projects</span></a>
                                </div>
                            </div>
                    </div>
                </div>
                <div class="wrap mcb-wrap one-second valign-top move-up clearfix animate" data-anim-type="fadeInLeft" style="margin-top:-571px" data-mobile="no-up">
                    <div class="mcb-wrap-inner">
                        <div class="column mcb-column one column_image">
                            <div class="image_frame image_item no_link scale-with-grid aligncenter no_border">
                                <div class="image_wrapper">
                                    <img class="scale-with-grid" src="~/assets/PlaneraInterior/images/about-section2.png">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="section mcb-section" style="padding-top:80px; padding-bottom:80px; background-color:#eeedec">
            <div class="section_wrapper mcb-section-inner interiorsteps">
                <div class="wrap mcb-wrap one-third valign-top clearfix">
                    <div class="mcb-wrap-inner">
                        <div class="column mcb-column one column_image">
                            <div class="image_frame image_item no_link scale-with-grid no_border">
                                <div class="image_wrapper">
                                    <img class="scale-with-grid" src="~/assets/PlaneraInterior/images/icon1.png" style="width:110px;">
                                </div>
                            </div>
                        </div>
                        <div class="column mcb-column one-sixth column_column">
                            <div class="column_attr clearfix">
                                <h2>1</h2>
                            </div>
                        </div>
                        <div class="column mcb-column two-third column_column">
                            <div class="column_attr clearfix">
                                <b>Briefing</b>, First thing you determine the concept and model you want.
                            </div>
                        </div>
                    </div>
                </div>
                <div class="wrap mcb-wrap one-third valign-top clearfix">
                    <div class="mcb-wrap-inner">
                        <div class="column mcb-column one column_image">
                            <div class="image_frame image_item no_link scale-with-grid no_border">
                                <div class="image_wrapper">
                                    <img class="scale-with-grid" src="~/assets/PlaneraInterior/images/icon3.png" style="width:110px;">
                                </div>
                            </div>
                        </div>
                        <div class="column mcb-column one-sixth column_column">
                            <div class="column_attr clearfix">
                                <h2>2</h2>
                            </div>
                        </div>
                        <div class="column mcb-column two-third column_column">
                            <div class="column_attr clearfix">
                                <b>Processing</b>, After briefing we carry out the execution of the desired concept.
                            </div>
                        </div>
                    </div>
                </div>
                <div class="wrap mcb-wrap one-third valign-top clearfix">
                    <div class="mcb-wrap-inner">
                        <div class="column mcb-column one column_image">
                            <div class="image_frame image_item no_link scale-with-grid no_border">
                                <div class="image_wrapper">
                                    <img class="scale-with-grid" src="~/assets/PlaneraInterior/images/icon2.png" style="width:110px;">
                                </div>
                            </div>
                        </div>
                        <div class="column mcb-column one-sixth column_column">
                            <div class="column_attr clearfix">
                                <h2>3</h2>
                            </div>
                        </div>
                        <div class="column mcb-column two-third column_column">
                            <div class="column_attr clearfix">
                                <b>Finishing</b>, After the process is complete, We will immediately carry out finishing stage.
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="section mcb-section" >
            <div class="section mcb-section center dark " style="background-image: url(../../assets/PlaneraInterior/images/about-section3.png); background-size: cover; background-repeat: no-repeat;" data-stellar-background-ratio="0.5">
                <div class="section_wrapper mcb-section-inner">
                    <div class="wrap mcb-wrap one valign-top clearfix">
                        <div class="mcb-wrap-inner">
                            <div class="column mcb-column one column_divider">
                                <hr class="no_line" style="margin:35px 0 ">
                            </div>
                            <div class="column mcb-column one-third column_column" style="margin-top:70px;">
                                <div class="column_attr clearfix  animate" data-anim-type="fadeInLeft" style="padding:0 10% 0 0;text-align:left;">
                                    <h2 style="color:white">
                                        Our <br />
                                            <span style="font-weight:300;opacity:0.4">Portfolio</span>
                                    </h2>
                                </div>
                            </div>
                            <div class="column mcb-column one-third column_column animate" style="margin-top:70px;" data-anim-type="fadeInRight">
                                <div class="column_attr clearfix" style="font-size:18px;padding:0 10% 0 0 ;text-align:left; color:lightgrey">
                                    Our completed projects carried to an extreme and become elegance. We aim to create aesthetic and sophisticated designs.
                                </div>
                            </div>

                            <div class="column mcb-column one-third column_button " style="display:grid">
                                <img src="~/assets/PlaneraInterior/images/vwcat.jpg" style="width:150px;margin-bottom:15px" />
                                <a class="button button_full_width button_size_2 button_js" href="~/Template/PLANERAINTERIOR.pdf" style="background:transparent;border:1px solid white;color:white;width:150px;margin-top:15px;" download><span class="button_label" style="padding: 10px">View Portfolio</span></a>
                            </div>
                            <div class="column mcb-column one column_divider">
                                <hr class="no_line" style="margin:35px 0 ">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="section mcb-section" style="padding-top:80px; padding-bottom:40px; ">
            <div class="section mcb-section" >
                <div class="section_wrapper mcb-section-inner">
                    <div class="wrap mcb-wrap one valign-top clearfix">
                        <div class="mcb-wrap-inner">
                            <div class="column mcb-column one column_column">
                                <div class="column_attr clearfix align_center" style="background-image: url('../../assets/PlaneraInterior/images/home_architect3_about3.png'); background-repeat: no-repeat; background-position: right bottom; padding: 80px 20% 65px; border: 20px solid #1f438f; border-radius: 6px">
                                    <h3 style="font-weight: 300">Your home should tell the story of who you are, and be a collection of what you love</h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section mcb-section" style="padding-top:20px; padding-bottom:80px;">
            <div class="section mcb-section" >
                <div class="wrap mcb-wrap one valign-top clearfix">
                    <div class="mcb-wrap-inner">
                        <div >
                            <div class="column_attr clearfix align_center">
                                <h2 style="padding-bottom:20px; ">OUR TEAM</h2>
                            </div>
                            <div class="wrapper">
                                <div class="sliding-background"></div>
                            </div>
                            <style>
                                .wrapper {
                                    overflow: hidden;
                                }

                                .sliding-background {
                                    background: url(../../assets/PlaneraInterior/images/Planera-Interior-Team-Slider.jpg) repeat-x;
                                    height: 535px;
                                    width: 5076px;
                                    animation: slide 30s linear infinite;
                                }

                                @@keyframes slide {
                                    0% {
                                        transform: translate3d(0, 0, 0);
                                    }

                                    100% {
                                        transform: translate3d(-1692px, 0, 0);
                                    }
                                }
                            </style>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
