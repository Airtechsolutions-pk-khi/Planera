                <li class="categories">
                    <a class="open" href="#" data-translate="00h7"><i class="icon-docs"></i>Categories<i class="icon-down-dir"></i></a>
                </li>
                <li class="reset">
                    <a class="close" data-rel="*" href="#" data-translate="0sa01"><i class="icon-cancel"></i> Show all</a>
                </li>
            </ul>
            <div class="filters_wrapper" style="display: block;">
                <ul class="categories">
                    <li class="current-cat">
                        <a data-rel=".category-home" href="#" data-translate="0jnb0">Janabiya</a>
                    </li>
                    <li>
