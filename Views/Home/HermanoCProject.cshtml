@{
    ViewBag.Title = "Hermano Contracting Project";
    Layout = "~/Views/Shared/_LayoutHermaniCL.cshtml";
}

<style>
    .if-zoom .image_frame .image_wrapper .image_links {
        display: unset;
    }

    .hc-projects img.scale-with-grid, #Content img {
        height: 251px;
    }

    .image_frame .image_wrapper .image_links, .portfolio_group.masonry-hover .portfolio-item .masonry-hover-wrapper .hover-desc {
        background: rgba(28, 130, 86, 0.80);
    }

        .image_frame .image_wrapper .image_links a:hover {
            background: #ffffff;
            color: rgba(28, 130, 86, 0.80) !important;
        }

    h5 {
        color: #056b39 !important;
    }
</style>
<div class="section mcb-section  equal-height-wrap full-width">
    <div class="section_wrapper mcb-section-inner">
        <div class="wrap mcb-wrap one  valign-top bg-cover clearfix" style=" padding-top: 167px; padding-bottom: 200px; background-image: url(../../assets/HarmanoRepair/images/project-header.jpg);background-repeat: no-repeat;background-position: center;">
            <div class="mcb-wrap-inner">
                <div class="column mcb-column one column_column ">
                    <div class="column_attr clearfix align_center">
                        <h2 style="margin: 0; color: white; font-weight: 800; font-size: 50px;" data-translate="0prj01">PROJECTS </h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
