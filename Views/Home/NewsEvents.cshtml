    ViewBag.Title = "Planeragroup/images";
    Layout = "~/Views/Shared/_Layoutv2.cshtml";
}
<style>
    div {
        font-family: 'Montserrat' !important;
    }

    .erinyen .tp-tab-desc {
        font-size: 16px;
    }
</style>
<div class="" style="height:100px; background-color: #090c36">

</div>
