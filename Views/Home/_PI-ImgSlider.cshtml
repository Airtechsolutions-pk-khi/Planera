                        <li data-index="rs-@count" data-transition="fade" data-slotamount="7" data-easein="default" data-easeout="default" data-masterspeed="300" data-thumb="../../assets/PlaneraInterior/images/janabiya-palace.jpg" data-rotate="0" data-saveperformance="off" data-title="@item.Title" data-param1="INTERIOR" data-description="">
                            <!-- MAIN IMAGE -->
                            @*<img src="~/assets/ActivarBH/slider/01.jpg" data-bgposition="30% 50%" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">*@

                            <img src="@System.Configuration.ConfigurationManager.AppSettings["Image"].ToString()@item.ImagePath" alt="@item.Title" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">

                        </li>
                    }
                    else
                    {
                        <li data-index="rs-@count" data-transition="fade" data-slotamount="7" data-easein="default" data-easeout="default" data-masterspeed="300" data-thumb="../../assets/PlaneraInterior/images/janabiya-palace.jpg" data-rotate="0" data-saveperformance="off" data-title="@item.ArabicTitle" data-param1="التصميم الداخلي" data-description="">
                            <!-- MAIN IMAGE -->
                            @*<img src="~/assets/ActivarBH/slider/01.jpg" data-bgposition="30% 50%" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">*@

                            <img src="@System.Configuration.ConfigurationManager.AppSettings["Image"].ToString()@item.ImagePath" alt="@item.ArabicTitle" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">

                        </li>
                    }
                    count += 1;
                }
            }

