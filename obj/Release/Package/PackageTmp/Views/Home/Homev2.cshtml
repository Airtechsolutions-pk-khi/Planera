<script>
    var $ = jQuery;
         function SendEmail() {
            var dataType = 'application/json; charset=utf-8';
            var obj = {
                "Name": jQuery('#name').val(),
                "Email": jQuery('#email').val(),
                "Subject": jQuery('#subject').val(),
                "Message": jQuery('#message').val()
            }
            jQuery.ajax({
                type: 'POST',
                url: '@Url.Action("SendEmailAdminPlaneraGroup", "Email")',
                dataType: 'json',
                contentType: dataType,
                data: JSON.stringify(obj),
                success: function (result) {
                    if (result.Success == true) {
                        swal({
                            title: "We have received you query, our team will get back to you soon!",
                            //text: "",
                            type: "success"
                        });
                        ClearFields()
                    }
                    else {
                        //alert("Error Occured While Sending an Email")
                        swal({
                            title: "Error !",
                            text: "Error Occured While Sending an Email",
                            type: "error",
                            html: true,
                            showCancelButton: false,
                            confirmButtonColor: "#DD6B55",
                            confirmButtonText: "Ok",
                            closeOnConfirm: false,
                        });
                        ClearFields()
                    }
                }
            });
        }
        function ClearFields() {
           jQuery('#name').val('');
           jQuery('#email').val('');
           jQuery('#subject').val('');
           jQuery('#message').val('');
        }
</script>